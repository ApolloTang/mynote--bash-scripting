     "text": [
      "$0: /usr/local/bin/bash\n",
      "$1: a\n",
      "$2: b\n",
      "${2-1}: b\n"
     ]
    }
   ],
   "source": [
    "function show_fruits(){\n",
    "    echo \"\\$0: $0\"\n",
    "    echo \"\\$1: $1\"\n",
    "    echo \"\\$2: $2\"\n",
    "    echo \"\\${2-1}: ${2-1}\"\n",
    "}\n",
    "show_fruits a b\n"
   ]
  },
